<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".TempActivity">

    <Button
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="사용자 메뉴 들어가기"
        android:id="@+id/gotouser"/>

    <Button
        app:layout_constraintTop_toBottomOf="@+id/gotouser"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="join 들어가기"
        android:id="@+id/gotojoin"/>

    <Button
        android:id="@+id/readDB"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="READ DB"
        app:layout_constraintTop_toBottomOf="@+id/gotojoin"
        tools:layout_editor_absoluteX="160dp" />

    <TextView
        android:id="@+id/dbContents"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="19dp"
        android:text="TextView"
        app:layout_constraintTop_toBottomOf="@+id/readDB"
        tools:layout_editor_absoluteX="175dp" />
</android.support.constraint.ConstraintLayout>